{
  "name": "set2020",
  "version": "0.0.1",
  "description": "Nest Backend from Boilerplate",
  "maintainers": [],
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nest start --watch",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "tscheck:w": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "hmr": "node dist/server",
    "webpack": "webpack --config webpack.config.js",
    "deploy:local": "docker-compose --env-file .env.development up -d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/typeorm": "^7.1.5",
    "crypto": "^1.0.1",
    "helmet": "^4.3.1",
    "mongodb": "^3.4.1",
    "nestjs": "^0.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/testing": "^7.6.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/joi": "^14.3.4",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "husky": "^4.0.7",
    "jest": "^24.9.0",
    "joi": "^14.3.1",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typescript": "^3.7.4",
    "webpack": "^5.12.3",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
